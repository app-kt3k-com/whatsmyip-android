<ivysettings>
    <resolvers>
        <ibiblio name="maven2" m2compatible="true" />

        <url name="kt3k" m2compatible="true">
            <artifact pattern="http://kt3k.github.io/repository/maven/release/[organization]/[module]/[revision]/[module]-[revision].jar" />
        </url>

        <chain name="chained" returnFirst="true">
            <resolver ref="maven2" />
            <resolver ref="kt3k" />
        </chain>
    </resolvers>

    <settings defaultResolver="chained" />
</ivysettings>
